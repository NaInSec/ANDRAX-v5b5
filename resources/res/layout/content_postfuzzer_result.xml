            <EditText android:textColor="@color/colorPrimary" android:textColorHint="@color/bluedack" android:id="@+id/editTextpostoutputbytes" android:layout_width="match_parent" android:layout_height="wrap_content" android:hint="Bytes [ 295 ]" android:ems="10" android:inputType="textPersonName"/>
